{
  "name": "lambda",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "scripts": {
    "deploy": "npm run copyContractJsonToSrcFolder && npm run transpileCodeToEs7 && sls deploy --stage dev",
    "offline": "npm run compileContracts && npm run copyContractJsonToSrcFolder && npm run transpileCodeToEs7 && sls offline start",
    "offlineQuick": "npm run transpileCodeToEs7 && sls offline start",
    "compileContracts": "cd ../ethereum/ && npm run compile && cd ../lambda",
    "copyContractJsonToSrcFolder": "cp -R ../ethereum/build/contracts/ ./lib/ethereum/build/contracts/",
    "transpileCodeToEs7": "babel ./src --out-dir ./lib",
    "compile": "babel -d lib/ src/",
    "compileRegistryPackage": "cd ../registry && npm run compile && cd ../lambda",
    "test": "npm run compileRegistryPackage && npm run compile && mocha --timeout 30000 --require babel-core/register --compilers js:babel-core/register --recursive || true"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.211.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "dotenv": "^5.0.1",
    "ethereumjs-util": "^5.1.5",
    "ethereumjs-wallet": "^0.6.0",
    "ipfs-mini": "^1.1.2",
    "scrypt": "^6.0.3",
    "serverless-plugin-include-dependencies": "^3.0.0",
    "truffle-contract": "^3.0.4",
    "truffle-wallet-provider": "0.0.5",
    "uuid": "^3.2.1",
    "web3-utils": "^1.0.0-beta.31"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "kudos-registry": "file:../registry",
    "mocha": "^5.0.4",
    "serverless-offline": "^3.18.0"
  }
}
