{
  "name": "lambda",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "scripts": {
    "deploy": "npm run compileContracts && npm run copyContractJsonToSrcFolder && npm run transpileCodeToEs7 && sls deploy --stage dev",
    "offline": "npm run compileContracts && npm run copyContractJsonToSrcFolder && npm run transpileCodeToEs7 && sls offline start",
    "compileContracts": "cd ../ethereum/ && truffle compile --all && cd ../lambda",
    "copyContractJsonToSrcFolder": "cp -R ../ethereum/build/contracts/ ./buildsrc/ethereum/build/contracts/",
    "transpileCodeToEs7": "babel ./src --out-dir ./buildsrc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.203.0",
    "axios": "^0.18.0",
    "dotenv": "^5.0.1",
    "ethereumjs-wallet": "^0.6.0",
    "ipfs-mini": "^1.1.2",
    "scrypt": "^6.0.3",
    "serverless-plugin-include-dependencies": "^3.0.0",
    "truffle-contract": "^3.0.3",
    "truffle-wallet-provider": "0.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "serverless-offline": "^3.18.0"
  }
}
